{
  "name": "graphy-daphy-backend",
  "version": "1.0.0",
  "description": "backend graphql api for graphy-daphy mvp",
  "main": "index.js",
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "dev": "nodemon dist/index.js",
    "watch": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-esm -d ./src/MigrationDataSource.ts",
    "typeorm:prod": "typeorm-ts-node-esm -d ./src/MigrationDataSourceProd.ts"
  },
  "author": "kareem shehab",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.18",
    "dotenv": "^16.0.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
